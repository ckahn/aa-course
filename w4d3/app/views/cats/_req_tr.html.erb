<tr>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
    <% if req.pending? %>
        <% if current_user && current_user.id == req.cat.user_id %>
          <%= button_to "Approve", approve_cat_rental_request_url(req) %>
          <%= button_to "Deny", deny_cat_rental_request_url(req) %>
        <% else %>
          <%= req.status %>
        <% end %>
    <% else %>
      <%= req.status %>
    <% end %>
  </td>
  <td>
    <%= req.requestor.user_name %>
  </td>
</tr>
